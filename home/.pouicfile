function proxy(){
  echo -e "\nSet proxy..."
  http_proxy=http://proxy-internet.localnet:3128
  https_proxy=https://proxy-internet.localnet:3128
  ftp_proxy=http://proxy-internet.localnet:3128
  non_proxy=localhost,127.0.0.1,.priv.atos.fr,.sock
  git config --global http.proxy http://proxy-internet.localnet:3128
  git config --global https.proxy https://proxy-internet.localnet:3128
  echo -e "\ndone"
}

function proxyoff(){
  echo -e "\nUnset proxy..."
  unset http_proxy;
  unset https_proxy
  unset ftp_proxy
  git config --global --unset http.proxy
  git config --global --unset https.proxy
  echo -e "\ndone"
}

